{
     "version": 2,
     "builds":[
         {
             "src": "./api/index.js",
             "use": "@vercel/node"
         }
     ],
     "rewrites": [
         {
               "source": "/(.*)",
               "destination": "api/index.js"
         }
     ],

     "routes": [
          {
               "src": "/api/auth/login",
               "dest": "/api/auth/login.js"
          }
     ]
}